ifneq ($(KBUILD_EXTMOD),)

EXTRA_CFLAGS += \
	-I$(TOPDIR)/include \
	-I$(TOPDIR)/drivers/net/can/hal \
	\
	-DETH_P_CAN=0x000C \
	-DARPHRD_CAN=280

